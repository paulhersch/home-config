{
    "DefaultDoc": {
        "prefix": "doc",
        "body": [
            "\\documentclass[a4paper, 11pt]{article}",
            "\\usepackage{amssymb}",
            "\\usepackage{amsmath}",
            "\\usepackage[ngerman]{babel}",
            "\\usepackage{fontspec}",
            "\\setmainfont{Inter}",
            "\\begin{document}",
            "$1",
            "\\end{document}"
        ],
        "description": "Default Document for simple Texts"
    },
    "CheatSheetDoc": {
        "prefix": "cheatdoc",
        "body": [
            "\\documentclass[9pt]{article}",
            "\\makeatletter",
            "\\renewcommand\\normalsize{\\@setfontsize\\large{7pt}{9}}",
            "\\renewcommand\\large{\\@setfontsize\\large{8pt}{10}}",
            "\\renewcommand\\Large{\\@setfontsize\\Large{9pt}{11}}",
            "\\renewcommand\\LARGE{\\@setfontsize\\Large{11pt}{12}}",
            "\\makeatother",
            "\\usepackage{amssymb}",
            "\\usepackage{amsmath}",
            "\\usepackage[ngerman]{babel}",
            "\\usepackage{fontspec}",
            "\\setmainfont{Inter}",
            "\\usepackage{multicol}",
            "\\setlength{\\columnseprule}{0.4pt}",
            "\\geometry{a4paper,left=5mm,right=5mm,top=5mm,bottom=5mm}",
            "\\usepackage{enumitem}",
            "\\setlist[itemize]{noitemsep, topsep=0pt}",
            "\\begin{document}",
            "\\begin{multicols}{3}",
            "$1",
            "\\end{multicols}",
            "\\end{document}"
        ],
        "description": "Default Document for Cheat Sheets"
    },
    "ItemizeHeading" : {
        "prefix": "itemizeheading",
        "body" : [
            "\\item[] \\textbf{$1}",
            "\\begin{itemize}",
            "   \\item $2",
            "\\end{itemize}"
        ]
    },
    "ItemizeConclusion" : {
        "prefix": "itemizeconclusion",
        "body" : [
            "\\item[\\$\\Rightarrow\\$] $1"
        ]
    },
    "InlineHeadingL5" : {
        "prefix": "h5",
        "body" : [
            "\\textbf{\\large{$1}}\\newline",
            "$2"
        ]
    },
    "InlineHeadingL4" : {
        "prefix": "h4",
        "body" : [
            "\\textbf{\\Large{$1}}\\newline",
            "$2"
        ]
    },
    "DefinitionBlock" : {
        "prefix": "block",
        "body": [
            "\\begin{block}{$1}",
            "$2",
            "\\end{block}"
        ]
    },
    "BigO" : {
        "prefix" : "bigOof",
        "body" : [
            "\\$\\mathcal{O}($1)\\$"
        ]
    }
}
