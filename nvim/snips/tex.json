{
    "DefaultDoc": {
        "prefix": "doc",
        "body": [
            "\\documentclass[a4paper, 11pt]{article}",
            "\\usepackage{amssymb}",
            "\\usepackage{amsmath}",
            "\\usepackage[ngerman]{babel}",
            "\\usepackage{fontspec}",
            "\\setmainfont{Inter}",
            "\\begin{document}",
            "$1",
            "\\end{document}"
        ],
        "description": "Default Document for simple Texts"
    },
    "CheatSheetDoc": {
        "prefix": "cheatdoc",
        "body": [
            "\\documentclass[9pt]{article}",
            "\\usepackage{amssymb}",
            "\\usepackage{amsmath}",
            "\\usepackage[ngerman]{babel}",
            "\\usepackage{fontspec}",
            "\\setmainfont{Inter}",
            "\\usepackage{multicol}",
            "\\setlength{\\columnseprule}{0.4pt}",
            "\\geometry{a4paper,left=5mm,right=5mm,top=5mm,bottom=5mm}",
            "\\let\\tempone\\itemize",
            "\\let\\temptwo\\enditemize",
            "\\renewenvironment{itemize}{\\tempone\\addtolength{\\itemsep}{\\dimexpr0pt-0.5\\baselineskip}}{\\temptwo}",
            "\\begin{document}",
            "\\begin{multicols}{3}",
            "$1",
            "\\end{multicols}",
            "\\end{document}"
        ],
        "description": "Default Document for Cheat Sheets"
    },
    "ItemizeHeading" : {
        "prefix": "itemizeheading",
        "body" : [
            "\\item[] \\textbf{$1}",
            "\\begin{itemize}",
            "   \\item $2",
            "\\end{itemize}"
        ]
    }
}
