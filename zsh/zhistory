\ 
echo \ 
echo \ a
subjectformat() {\
        echo $1\
        betreff=$1\
        echo $betreff | grep 'utf-8?' >/dev/null && {\
                echo $betreff | grep 'utf-8?[Bb]' >/dev/null && {\
                        #das =? ist eine SicherheitsmaÃƒ¿nahme, falls der String von Exim abgeschnitten wird\
                        echo "${betreff}=?" | awk -F'?' '{ print $4 }' | base64 -d\
                } || {\
                        echo $(utf8_dec $betreff)\
                }\
        } || echo $betreff\
}
subjectformat() {\
        betreff=$1\
        echo $betreff | grep 'utf-8?' >/dev/null && {\
                echo $betreff | grep 'utf-8?[Bb]' >/dev/null && {\
                        echo "${betreff}=?" | awk -F'?' '{ print $4 }' | base64 -d\
                } || {\
                        echo $(utf8_dec $betreff)\
                }\
        } || echo $betreff\
}
subjectformat '=?utf-8?B?RXJpbm5lcnVuZzogVmVydHJhZyBmw7xyICJBZHZpc29yeSBCb2FyZCBuZXVlIFZlcnM=?='
subjectformat 'VmVyd2VpZ2VydW5nIGRlciBWZXJsw6RuZ2VydW5nIElocmVzIERvbWFpbm5hbWU='
subjectformat '=?utf-8?b?VmVyd2VpZ2VydW5nIGRlciBWZXJsw6RuZ2VydW5nIElocmVzIERvbWFpbm5hbWU=?='
subjectformat "asdasd"
